        MsgBox strmsj, vbCritical + vbOKOnly, Me.Caption
        If conexion.State = adStateOpen Then conexion.Close
        If rs.State = adStateOpen Then rs.Close
        Set conexion = Nothing
        Set rs = Nothing
        Val10PDescCliForaneoNue = False
    End Function
