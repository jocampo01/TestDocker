       blnCatalogo = True
        If frmMenuCliente.Visible Then
            txtCliente = ""
            txtCliente_KeyDown vbKeyReturn, 1
            'ReajustaColorOpcion lblCatClientes, 2, 2
        End If
        blnCatalogo = False
        MousePointer = vbArrow
        Exit Sub
ErrVB:
        MousePointer = vbArrow
    End Sub
    
