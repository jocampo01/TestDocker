        Dim lRS As New Recordset
        Dim lCN As New Connection
        Dim lstrSql As String
        Dim lstrConfigDecimales As String
        'ROl005-I
        Dim LstrBorraLog As String
        Dim LstrFechaBorraLog As String
        'ROl005-F
    
        '------------------
    'edma
    flag = 1
